pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
	back = {}     // i decided this would be way less confusing
		back.x = 0   // if i just like, retroactively add a wall behind you
		back.y = 117 // but didnt call it 'screen.x2' or try and rename all screen.x)
	screen = {}
		screen.x = 117
		screen.y = 0
	floor = {}
		floor.y = 117
	z = {}
		z.frame = 0
		z.speed = -4
		z.decay = 0
	x = {}
		x.frame = 0
  result = false
	player = {}
		player.x = 10
		player.y = 110
		player.x_speed = 0
		player.y_speed = 0
		player.x_velocity = 0
		player.y_velocity = 0
		player.sprite = 1
		player.sprite_max = 7
		player.sprite_min = 4
		player.sprite_last = 4
  player.aligned = screen.x-60
  player.floored = false
  player.invuln = 0
	counter = 0
 orbs = {}
	spikes = {}
 score = 0
 game_state = 0
end

// controls/gameplay

function _update()
	if game_state == 0 then
		if start_game(btnp(4)) then
			game_state = 1
		end
	elseif game_state == 1 then
  result = jump_player(btn(4))
  move_player()
  gravity_player()
  vuln_player()
  create_orbs()
  move_orbs()
  create_spikes()
  move_spikes()
  collide_spikes_player()
  collide_orbs_player()
  shrink_screen()
  if end_game() then
 		game_state = 2
 	end
	end
end

function start_game(pressed)
	if pressed then
		return true
	else
		return false
	end
end

function end_game()
	if screen.x < 18 then
	 sfx(2)
		return true
	else
		return false
	end
	
end


function vuln_player()
 if player.invuln > 0 and counter % 5 == 0 then 
 	player.invuln -= 1
 	if player.sprite >= 7 then
 		player.sprite = 4
 	else
 		player.sprite = 3
 	end
 end
end

function create_spikes()
	// orbs contents
	// 1 left x value
	//	2 left y value
	// 3 middle x value
	// 4 middle y value
	// 5 right x value
	// 6 right y value
	local x2, x3, y2
	x2 = (screen.x-1)+(rnd(6)+2)
	x3 = x2+(rnd(6)+2)
	y2 = (floor.y-1)-(rnd(6)+1)
	if counter % 80 == 25 then
		add(spikes, {screen.x-1, floor.y-1, x2, y2, x3, floor.y-1}) 
	end
end

function move_spikes()
	for spike in all(spikes) do
		if spike[5] < back.x+11 then
			del(spikes, spike)
		else
			spike[1] -= 1
			spike[3] -= 1
			spike[5] -= 1
		end
	end
end

function collide_spikes_player()
	local y = 0
	for spike in all(spikes) do
		y = spike[4]
		repeat
			if player.invuln <= 0 and player.x >= spike[1] and player.x+4 <= spike[5] then
				if player.y+8 >= y then
				sfx(0)
				screen.x -= 10
				screen.y += 10
				player.y_speed -= 2
				player.x_speed += 5
				player.invuln = 10
				end
			end
			y += 1
		until y > floor.y
	end
end

function shrink_screen()
	if counter % 8 == 4 then
	 screen.x -= 1
	 screen.y += 1
	 if player.aligned >= 10 then
	 	player.aligned = screen.x-60
	 end
	 score += 1
	end
end

function create_orbs()
	// orbs contents
	// 1 sprite
	// 2 sprite_min
	// 3 sprite_max
	// 4 x
	// 5 y
	if counter % 60 == 0 then
		add(orbs, {16, 16, 23, screen.x-8, rnd(30)+80})
	end
end

function move_orbs()
	for orb in all(orbs) do
		if orb[4] < back.x+10 then
			del(orbs, orb)
		else
			orb[4] -= 1
		end
	end
end

function collide_orbs_player()
	local hitbox = {player.x, player.y, player.x+8, player.y+8}
	for orb in all(orbs) do
		local hitorb = {orb[4], orb[5], orb[4]+8, orb[5]+8}
		if collision(hitbox, hitorb) then
			del(orbs, orb)
			sfx(3)
			score += 10
		 if screen.x > 88 then
		 	screen.x = 117
		 	screen.y = 0
		 else
		 	screen.x += 30
		 	screen.y -= 30
		 end
		 if player.aligned > 10 then
			 player.aligned = screen.x-60
			end
		end
	end
end

function jump_player(jump)
  if jump and player.floored then
		z.frame += 1
		sfx(1)
		if (z.speed < z.decay) then
			player.y_velocity = z.decay
			player.y_speed = z.decay
      z.decay -= 1
		end
    return true
  elseif jump and not player.floored then
    z.frame += 1
    return true
	elseif not jump then
		z.frame = 0
		z.decay = 0
    return false
	end
end

function move_player()		
	// align player with 'middle'
	if player.aligned < 10 then
			player.aligned = 10
	end
	
	if player.y == floor.y-8 and player.x != player.aligned then
		if counter % 3 == 0 then
			if player.x > player.aligned then
				player.x_speed = -1
			else
				if player.x_speed > 1 then
					player.x_speed -= 1
				else
					player.x_speed = 1
				end
			end
		end
	else
		player.x_speed = 0
	end
	
	// move player
	player.y_speed += player.y_velocity/50
	player.y += player.y_speed
 player.x += player.x_speed
	
	// ensure player cannot go lower than floor at floor
	if player.y > floor.y-8 then
  	player.y = floor.y-8
	end
	
	// ensure player go higher than ceiling
	if player.y < screen.y+10 then
		player.y = screen.y+10
	end
	
end

function gravity_player()
  // gravity
	if player.y < floor.y-8 then
		player.y_velocity += 1.2
    player.floored = false
  else
    player.y_velocity = 0
    player.y_speed = 0
    player.floored = true
	end
end

//
// visuals
//

function _draw()
	counter += 1
	cls()
	if game_state == 0 then
		draw_title()
	elseif game_state == 1 then
		draw_sky()
		draw_floor()
		draw_orbs()
		draw_spikes()
		draw_player()
		draw_debug()
	elseif game_state == 2 then
	
	end
end

function draw_title()
	rect(40, 40, 88, 80, 7)
	rect(32, 32, 96, 88, 7)
	// 'cubed'
	spr(34, 44, 48)
	spr(35, 52, 48)
	spr(36, 60, 48)
	spr(37, 68, 48)
	spr(38, 76, 48)
	// 'press'
	spr(50, 44, 64)
	spr(51, 52, 64)
	spr(37, 60, 64)
	spr(52, 68, 64)
	spr(52, 76, 64)
	// 🅾️
	spr(53, 60, 56)
end

function draw_spikes()
	local y = 0
	for spike in all(spikes) do
		y = spike[4]
		if spike[3] > screen.x then
			repeat
				line(spike[1], spike[2], screen.x-1, y)
				y += 1
			until y >	floor.y
		elseif spike[5] > screen.x then
			repeat 
			 line(spike[1], spike[2], spike[3], y)
			 line(spike[3], y, screen.x-1, spike[6])
			 y += 1
		 until y > floor.y	 
		elseif spike[1] < back.x+10 and spike[3] > back.x+10 then
		 repeat
			 line(back.x+11, spike[2], spike[3], y)
			 line(spike[3], y, spike[5], spike[6])
		  y += 1
			until y > floor.y			 
		elseif spike[3] < back.x+10 and spike[5] > back.x+10 then
			repeat 
				line(back.x+11, y, spike[5], spike[6])
			 y += 1
			until y > floor.y
		else
	  repeat
			 line(spike[1], spike[2], spike[3], y)
			 line(spike[3], y, spike[5], spike[6]) 
			 y += 1
			until y > floor.y
		end
	end
end

function draw_sky()
	rectfill(back.x+11, screen.y+11, screen.x-1, floor.y-1, 5)
end

function draw_debug()
	print("score: "..score) 
end

function draw_player()

	// figure out the players frame
	if counter % 5 == 0 and player.floored then
		player.sprite += 1
		if player.sprite > player.sprite_max then
			player.sprite = player.sprite_min
		end
	end
	
		// literally draw the player
	spr(player.sprite, player.x, player.y)
end

function draw_floor()
	// literally draw the floor and walls
	color(7)
	// left wall
	line(back.x, back.y+10, back.x, screen.y)
	line(back.x+10, back.y, back.x+10, screen.y+10)
	// floor
	line(back.x+10, floor.y, screen.x, floor.y)
	line(back.x, floor.y+10, screen.x+10, floor.y+10)
	// ceiling
	line(back.x, screen.y, screen.x+10, screen.y)
	line(back.x+10, screen.y+10, screen.x, screen.y+10)
	// right wall
	line(screen.x, floor.y, screen.x, screen.y+10)
	line(screen.x+10, floor.y+10, screen.x+10, screen.y)
end

function draw_orbs()
	for orb in all(orbs) do
		spr(orb[1], orb[4], orb[5])
		if counter % 5 == 2 then
			orb[1] += 1
		end
		if orb[1] > orb[3] then
			orb[1] = orb[2]
		end
	end
end

// utility

function collision(a, b)
	c = {(a[3]-a[1])/2+a[1], (a[4]-a[2])/2+a[2]}
	if c[1] > b[1] and c[1] < b[3] then
	 if c[2] > b[2] and c[2] < b[4] then
	  return true
	 else
	  return false
	 end
	else
		return false
	end
end
